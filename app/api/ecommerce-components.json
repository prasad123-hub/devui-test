        "content": "import { Heart, Trash } from 'lucide-react'\nimport React from 'react'\n\nconst products = [\n  {\n    id: 1,\n    name: 'Nike Air Force 1 07 LV8',\n    href: '#',\n    price: '₹47,199',\n    originalPrice: '₹48,900',\n    discount: '5% Off',\n    color: 'Orange',\n    size: '8 UK',\n    imageSrc:\n      'https://static.nike.com/a/images/c_limit,w_592,f_auto/t_product_v1/54a510de-a406-41b2-8d62-7f8c587c9a7e/air-force-1-07-lv8-shoes-9KwrSk.png',\n  },\n  {\n    id: 2,\n    name: 'Nike Blazer Low 77 SE',\n    href: '#',\n    price: '₹1,549',\n    originalPrice: '₹2,499',\n    discount: '38% off',\n    color: 'White',\n    leadTime: '3-4 weeks',\n    size: '8 UK',\n    imageSrc:\n      'https://static.nike.com/a/images/c_limit,w_592,f_auto/t_product_v1/e48d6035-bd8a-4747-9fa1-04ea596bb074/blazer-low-77-se-shoes-0w2HHV.png',\n  },\n  {\n    id: 3,\n    name: 'Nike Air Max 90',\n    href: '#',\n    price: '₹2219 ',\n    originalPrice: '₹999',\n    discount: '78% off',\n    color: 'Black',\n    imageSrc:\n      'https://static.nike.com/a/images/c_limit,w_592,f_auto/t_product_v1/fd17b420-b388-4c8a-aaaa-e0a98ddf175f/dunk-high-retro-shoe-DdRmMZ.png',\n  },\n]\n\nexport function CartTwo() {\n  return (\n    <div className=\"mx-auto max-w-7xl px-2 lg:px-0\">\n      <div className=\"mx-auto max-w-2xl py-8 lg:max-w-7xl\">\n        <h1 className=\"text-3xl font-bold tracking-tight text-gray-900 sm:text-4xl\">\n          Shopping Cart\n        </h1>\n        <form className=\"mt-12 lg:grid lg:grid-cols-12 lg:items-start lg:gap-x-12 xl:gap-x-16\">\n          <section aria-labelledby=\"cart-heading\" className=\"rounded-lg bg-white lg:col-span-8\">\n            <h2 id=\"cart-heading\" className=\"sr-only\">\n              Items in your shopping cart\n            </h2>\n            <ul role=\"list\" className=\"divide-y divide-gray-200\">\n              {products.map((product, productIdx) => (\n                <div key={product.id} className=\"\">\n                  <li className=\"flex py-6 sm:py-6 \">\n                    <div className=\"flex-shrink-0\">\n                      <img\n                        src={product.imageSrc}\n                        alt={product.name}\n                        className=\"sm:h-38 sm:w-38 h-24 w-24 rounded-md object-contain object-center\"\n                      />\n                    </div>\n\n                    <div className=\"ml-4 flex flex-1 flex-col justify-between sm:ml-6\">\n                      <div className=\"relative pr-9 sm:grid sm:grid-cols-2 sm:gap-x-6 sm:pr-0\">\n                        <div>\n                          <div className=\"flex justify-between\">\n                            <h3 className=\"text-sm\">\n                              <a href={product.href} className=\"font-semibold text-black\">\n                                {product.name}\n                              </a>\n                            </h3>\n                          </div>\n                          <div className=\"mt-1 flex text-sm\">\n                            <p className=\"text-sm text-gray-500\">{product.color}</p>\n                            {product.size ? (\n                              <p className=\"ml-4 border-l border-gray-200 pl-4 text-sm text-gray-500\">\n                                {product.size}\n                              </p>\n                            ) : null}\n                          </div>\n                          <div className=\"mt-1 flex items-end\">\n                            <p className=\"text-xs font-medium text-gray-500 line-through\">\n                              {product.originalPrice}\n                            </p>\n                            <p className=\"text-sm font-medium text-gray-900\">\n                              &nbsp;&nbsp;{product.price}\n                            </p>\n                            &nbsp;&nbsp;\n                            <p className=\"text-sm font-medium text-green-500\">{product.discount}</p>\n                          </div>\n                        </div>\n                      </div>\n                    </div>\n                  </li>\n                  <div className=\"mb-2 flex\">\n                    <div className=\"min-w-24 flex\">\n                      <button type=\"button\" className=\"h-7 w-7\">\n                        -\n                      </button>\n                      <input\n                        type=\"text\"\n                        className=\"mx-1 h-7 w-9 rounded-md border text-center\"\n                        value=\"1\"\n                      />\n                      <button type=\"button\" className=\"flex h-7 w-7 items-center justify-center\">\n                        +\n                      </button>\n                    </div>\n                    <div className=\"ml-6 flex text-sm\">\n                      <button type=\"button\" className=\"flex items-center space-x-1 px-2 py-1 pl-0\">\n                        <Trash size={12} className=\"text-red-500\" />\n                        <span className=\"text-xs font-medium text-red-500\">Remove</span>\n                      </button>\n                    </div>\n                  </div>\n                </div>\n              ))}\n            </ul>\n          </section>\n          {/* Order summary */}\n          <section\n            aria-labelledby=\"summary-heading\"\n            className=\"mt-16 rounded-md bg-white lg:col-span-4 lg:mt-0 lg:p-0\"\n          >\n            <h2\n              id=\"summary-heading\"\n              className=\" border-b border-gray-200 px-4 py-3 text-lg font-medium text-gray-900 sm:p-4\"\n            >\n              Price Details\n            </h2>\n            <div>\n              <dl className=\" space-y-1 px-2 py-4\">\n                <div className=\"flex items-center justify-between\">\n                  <dt className=\"text-sm text-gray-800\">Price (3 item)</dt>\n                  <dd className=\"text-sm font-medium text-gray-900\">₹ 52,398</dd>\n                </div>\n                <div className=\"flex items-center justify-between pt-4\">\n                  <dt className=\"flex items-center text-sm text-gray-800\">\n                    <span>Discount</span>\n                  </dt>\n                  <dd className=\"text-sm font-medium text-green-700\">- ₹ 3,431</dd>\n                </div>\n                <div className=\"flex items-center justify-between py-4\">\n                  <dt className=\"flex text-sm text-gray-800\">\n                    <span>Delivery Charges</span>\n                  </dt>\n                  <dd className=\"text-sm font-medium text-green-700\">Free</dd>\n                </div>\n                <div className=\"flex items-center justify-between border-y border-dashed py-4 \">\n                  <dt className=\"text-base font-medium text-gray-900\">Total Amount</dt>\n                  <dd className=\"text-base font-medium text-gray-900\">₹ 48,967</dd>\n                </div>\n              </dl>\n              <div className=\"px-2 pb-4 font-medium text-green-700\">\n                You will save ₹ 3,431 on this order\n              </div>\n            </div>\n          </section>\n        </form>\n      </div>\n    </div>\n  )\n}\n"
